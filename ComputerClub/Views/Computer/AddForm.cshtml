@using ComputerClub.ViewModels
@model ComputerViewModel

@{
    ViewData["Title"] = "Add User";
}

<form method="post" asp-action="InsertInto">
    <div class="form-group">
        <label>Model name</label>
        <input asp-for="ModelName" class="form-control" id="exampleInputPassword1" placeholder="Enter Model name" required>
    </div>
    <div class="form-group">
        <label>Price For Hour</label>
        <input asp-for="PriceForHour" class="form-control" id="exampleInputPassword1" placeholder="Enter price for hour" required>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
